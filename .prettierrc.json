{
  "printWidth": 100,
  "singleQuote": true,
  "trailingComma": "all",
  "endOfLine": "lf",
  "importOrder": [
    "^(react)",
    "^((?!/).)*$",
    "^(@tanstack/*)(.*|$)*$",
    "^(@redux/*|redux/*)(.*|$)*$",
    "^((.*)/types)(.*|$)",
    "^((hooks))(.*|$)",
    "^(pages)(.*|$)",
    "^(./components|components)*$",
    "^(./components)(.*|$)",
    "^(./styles)(.*|$)",
    "^(.)(.*|$)",
    "^(..)(.*|$)"
  ],
  "importOrderSeparation": true
}
